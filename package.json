{
	"name": "evencio-studio",
	"private": false,
	"description": "Evencio Studio - source-available design studio for event organizers",
	"license": "FSL-1.1-MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/evenc-io/evencio-studio.git"
	},
	"keywords": [
		"marketing",
		"events",
		"social-media",
		"poster",
		"design",
		"tanstack",
		"bun"
	],
	"author": "Yan Malinovskiy <yanmalinovskiy@evenc.io>",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3010",
		"build": "bun --bun vite build",
		"start": "bun run .output/server/index.mjs",
		"preview": "vite preview",
		"test": "bun test",
		"verify": "bun run lint && bun run test && bun run build && bun run e2e",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"e2e:headed": "playwright test --headed",
		"lint": "biome check . && tsc --noEmit",
		"lint:fix": "biome check --fix .",
		"format": "biome format --write .",
		"perf:wasm": "PERF_WASM=1 bun test src/lib/wasm/__tests__/snippet-wasm.test.ts",
		"wasm:snippets": "ZIG_GLOBAL_CACHE_DIR=.zig-cache ZIG_LOCAL_CACHE_DIR=.zig-cache zig build-exe scripts/wasm/snippet_wasm.zig -O ReleaseSmall -target wasm32-freestanding -fno-entry -rdynamic -femit-bin=src/lib/wasm/snippet_wasm.wasm",
		"third-party-notices": "node scripts/generate-third-party-notices.mjs"
	},
	"dependencies": {
		"@babel/parser": "^7.28.5",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@hookform/resolvers": "^5.2.2",
		"@hugeicons/react": "^1.1.4",
		"@monaco-editor/react": "^4.7.0",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tabs": "^1.1.13",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-query": "^5.90.14",
		"@tanstack/react-router": "1.143.11",
		"@tanstack/react-router-ssr-query": "1.143.11",
		"@tanstack/react-start": "1.143.11",
		"@tanstack/react-virtual": "^3.13.17",
		"@tanstack/router-plugin": "1.143.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"esbuild-wasm": "^0.25.12",
		"fabric": "^7.0.0",
		"html-to-image": "^1.11.13",
		"idb": "^8.0.3",
		"jspdf": "^3.0.4",
		"lucide-react": "^0.562.0",
		"monaco-editor": "^0.55.1",
		"nanoid": "^5.1.6",
		"nitro": "latest",
		"playwright": "^1.57.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.69.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"three": "^0.182.0",
		"vite-tsconfig-paths": "^6.0.3",
		"zod": "^4.2.1",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@happy-dom/global-registrator": "^20.0.11",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@types/bun": "^1.3.5",
		"@types/node": "^22.19.3",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/three": "^0.182.0",
		"@vitejs/plugin-react": "^5.1.2",
		"fake-indexeddb": "^6.2.5",
		"jsdom": "^27.4.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.0",
		"web-vitals": "^5.1.0"
	}
}
